{
  "manifest_version": 2,
  "name": "Resize the Window",
  "version": "0.0.1",
  "description": "Resize the Window",
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Resize the Window",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "images/Icon-16.png",
    "48": "images/Icon-48.png",
   "128": "images/Icon-128.png"
  }
}